/* pages/mine/collect/collect.wxss */
.box .cartBox {
  display: flex;
  overflow: hidden;
}

.box .cartBox movable-area {
  width: 750rpx;
  height: 240rpx;
}

.box .cartBox movable-area movable-view {
  width: 950rpx;
  height: 240rpx;
}

.box .cartBox .main {
  width: 750rpx;
  height: 240rpx;
  display: inline-flex;
  margin-left: 15rpx;

}

.box .cartBox .main .delete {
  background-color: red;
  height: 220rpx;
  line-height: 220rpx;
  width: 200rpx;
  padding-left: 15rpx;
  flex-shrink: 0;
}

.box .cartBox .main .delete text {
  color: white;
  font-size: 40rpx;
}


.box .cartBox .main .img {
  height: 80%;
  width: 196rpx;
  border: 1px solid grey;
  margin-top: 24rpx;
}

.box .cartBox .main .img image {
  height: 80%;
  width: 196rpx;
}

.box .cartBox .main .cartContent {
  margin-top: 24rpx;
  width: 520rpx;
  height: 80%;
  display: inline-block;
  margin-left: 5%;
  margin-right: 5%;
}


.box .cartBox .main .cartContent .cartitle {
  color: grey;
  font-size: 24rpx;
  height: 140rpx;
  width: 480rpx;
}

.box .cartBox .main .cartContent .price_num {
  width: 100%;
  height: 56rpx;
  display: inline-flex;
}

.box .cartBox .main .addCart{
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  left: 70%;
  top: 65%;
}
.box .cartBox .main .addCart image{
  width: 56rpx;
  height: 56rpx;
}
.else .cartPng {
  margin-top: 160rpx;
  text-align: center;
  justify-content: center;
  display: flex;
}

.else .cartPng image {
  height: 100rpx;
  width: 100rpx;
}

.else .empty {
  margin-top: 30rpx;
  text-align: center;
  justify-content: center;
  display: flex;
  color: grey;
}

.else button {
  margin-top: 30rpx;
  text-align: center;
  justify-content: center;
  display: flex;
  color: grey;
}