page{
  background-color:#ced6e0;
}
.box {
  width: 100% ;
  height: 100%;
  padding: 15rpx;
}
.box .addressd {
  padding-top: 15rpx;
  height: 140rpx;
  width: 81%;
  background-color: white;
  padding-left: 15%;
  border-radius: 25rpx;
}
.box .addressd .city {
  font-size: 25rpx;
}
.box .addressd .road {
  font-size: 40rpx;
  font-weight: bold;
}
.box .addressd .name {
  font-size: 25rpx;
}
.box .addressd .alter {
  font-size: 25rpx;
  position: absolute;
  left: 600rpx;
  top: 34rpx;
  color: red;
}
.box .addressd .img {
 position: absolute;
 left: 30rpx;
 top: 50rpx;
}
.box .addressd .img image{
  width: 80rpx;
  height: 80rpx;
}
.box .products {
  margin-top: 20rpx;
  border-radius: 25rpx;
  height: 100%;
  width: 100%;
}
.box .products .products_item {
  padding-top: 15rpx;
  height: 180rpx;
  width: 93%;
  background-color: white;
  padding-left: 3%;
  display: flex;
}
.box .products .products_item .pic {
  height: 160rpx;
  width: 160rpx;
}
.box .products .products_item .pic image {
  width: 160rpx;
  height: 160rpx;
}
.box .products .products_item .title {
  height: 160rpx;
  width: 420rpx;
  color: grey;

  margin-left: 25rpx;
  font-size: 25rpx;
}
.box .products .products_item .price_num {
  height: 160rpx;
  width: 100rpx;
  padding-right: 10rpx;
}
.box .products .products_item .price_num .price {
  height: 50rpx;
  text-align: right;
}
.box .products .products_item .price_num .num {
  height: 110rpx;
  text-align: right;
}
.buttom {
  border-top: solid #eee thin;
  position: fixed;
  bottom: 0;
  display: flex;
  width: 100%;
  height: 120rpx;
  font-size: 24rpx;
}

.buttom .in {
  border-bottom: solid #eee thin;
  width: 95%;
  height: 80rpx;
  display: flex;
  line-height: 80rpx;
  padding-left: 5%;
}

.buttom .in .pay {
  height: 80rpx;
  width: 25%;
  position: absolute;
  right: 0;
  text-align: center;
  color: white;
  font-size: 30rpx;
}

.else .cartPng {
  margin-top: 160rpx;
  text-align: center;
  justify-content: center;
  display: flex;
}

.else .cartPng image {
  height: 100rpx;
  width: 100rpx;
}
.else .empty{
  margin-top: 30rpx;
  text-align: center;
  justify-content: center;
  display: flex;
  color: grey;
}
.else button{
  margin-top: 30rpx;
  text-align: center;
  justify-content: center;
  display: flex;
  color: grey;
}