/* pages/shop/shop.wxss */

.switchBox swiper {
  height: 340rpx;
  width: 750rpx;
}
.switchBox swiper image {
  width: 100%;
}
.casteList {
  display: flex;
}
.casteList navigator {
  padding: 20rpx;
  flex: 1;
}
.casteList navigator image {
  width: 100%;
}
.floor image {
  width: 100%;
}
.floor .product_list navigator {
  float: left;
  width: 33%;
}
.floor .product_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 2rpx solid white;
}
.floor .product_list navigator:nth-child(2),
.floor .product_list navigator:nth-child(3) {
  border-bottom: 1px solid white ;
}
.floor .product_list navigator image {
  width: 100%;
  height: 100%;
}